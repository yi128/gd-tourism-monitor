{
    "version": 2,
    "name": "guangdong-tourism-dashboard",
    "builds": [
        {
            "src": "dist/**",
            "use": "@vercel/static"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/dist/$1"
        }
    ],
    "buildCommand": "npm run build",
    "outputDirectory": "dist"
}